}\n\n.box__content-imageNav {\n  max-width: 100%;\n}\n\n.mask {\n  mask-size: 100%;\n  -webkit-mask-image: url(../../images/mask-alpha.png);\n  mask-image: url(../../images/mask-alpha.png);\n  mask-mode: alpha;\n}\n\n/* Destacar enlace al pasar mouse */\n\n.box__container a {\n  color: black;\n  text-decoration: none;\n}\n\n.box__container a:hover {\n  color: yellow;\n}\n"},"lineCount":null}},"error":null,"hash":"482f76497a0d06a40a5d4150efb18d58","cacheData":{"env":{}}}