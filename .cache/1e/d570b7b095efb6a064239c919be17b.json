ize: 7vw;\n    width: 90%;\n    right: 0;\n    text-align: center;\n  }\n\n  /* ordenar los div según el orden especificado en los requisitos */\n\n  .header__logo {\n    order: 2;\n    width: 60%;\n    left: 10%;\n  }\n\n  .header__img1 {\n    width: 25%;\n  }\n\n  .header__img2 {\n    width: 7%;\n  }\n\n  .header__img3 {\n    width: 10em;\n  }\n\n  @keyframes logoBook {\n    0% {\n      opacity: 0;\n    }\n\n    25% {\n      opacity: 0.5;\n      left: 5%;\n    }\n  \n    50% {\n      opacity: 0.9;\n      left: 15%;\n    }\n\n    100% {\n      opacity: 1;\n      left: 20%;\n    }\n  }\n  \n  @keyframes logoGlasses {\n    0% {\n      opacity: 0;\n      left: 10%;\n    }\n\n    25% {\n      opacity: 0.5;\n      left: 15%;\n    }\n  \n    50% {\n      opacity: 9;\n      left: 19%;\n    }\n\n    100% {\n      left: 13%;\n      top: 15.5em;\n    }\n  }\n\n  @keyframes logoText {\n    0% {\n      opacity: 0;\n      left: 13%;\n      top: 21em;\n    }\n\n    25% {\n      opacity: 0.2;\n    }\n\n    50% {\n      opacity: 0.7;\n    }\n\n    100% {\n      opacity: 1;\n      left: 13%;\n      top: 21em;\n    }\n  }\n\n  .header__img {\n    position: relative;\n    margin: 0% 0% 0% -5%;\n    width: 20%;\n  }\n\n  .header__menu-nav {\n    position: relative;\n    order: 3;\n    width: 50%;\n    margin-top: 15em;\n    left: 40%;\n    bottom: 20em;\n    margin-bottom: -17em;\n  }\n\n  /* Estilo de footer */\n\n  .container_footer {\n    font-size: 2.5em;\n  }\n\n  .footer_title {\n    font-size: 1.5em;\n  }\n\n  /* estilo de párrafo de descripción de lo que es la narrativa */\n\n  .box {\n    text-align: center;\n  }\n\n  .box__title {\n    text-align: center;\n    margin: auto;\n    background-color: lightblue;\n    width: 90%;\n    font-size: 3.5em;\n  }\n\n  /* colocación de div en modo vertical contenido de index.html */\n\n  .box__container {\n    left: 0;\n    font-size: 2em;\n    width: 70%;\n  }\n\n  .box__container-description {\n    width: 80%;\n    left: 0;\n    font-size: 2em;\n    text-align: justify;\n  }\n\n  /* Estilo para categorías */\n\n  .box__text {\n    font-size: 2em;\n    width: 80%;\n  }\n\n  .box__name {\n    font-size: 1.5em;\n  }\n\n  .box__content-image-book {\n    margin: auto;\n    width: 75%;\n    text-align: center;\n    margin-top: 4%;\n  }\n\n  .box__content-imageLibro {\n    height: 25em;\n  }\n\n  .box__content-imageAutor {\n    width: 20%;\n  }\n\n  /* Estilo para details */\n\n  .box__details-respon {\n    flex-direction: column;\n  }\n\n  .box__container-book-respon {\n    display: flex;\n  }\n\n  .box__container-book {\n    position: relative;\n    left: 10%;\n    width: 100%;\n  }\n  \n  .box__image-details {\n    width: 45%;\n    height: 10%;\n  }\n  \n  .box__buy {\n    width: 30%;\n    font-size: 2.5em;\n    height: 4.5em;\n    margin-top: 10%;\n  }\n\n  .box__container-description-book {\n    width: 100%;\n    font-size: 2em;\n  }\n\n  .container_gif,\n  .container_svg {\n    height: 20vw;\n  }\n\n  .other-img {\n    width: 100%;\n  }\n\n  .container_svg {\n    width: 40%;\n  }\n  \n  .container_gif {\n    width: 20%;\n  }\n\n  .img_gif {\n    height: 100%;\n  }\n\n  .box__name-autor {\n    font-size: 1.5em;\n  }\n\n  .box__summary {\n    font-size: 1em;\n  }\n\n  .box__slide {\n    font-size: 5em;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"db3afc7563abb328a48c6f6aa4723bc9","cacheData":{"env":{}}}